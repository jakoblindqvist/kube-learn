{
    "metrics": [
        {
            "name": "node_cpu_norm",
            "flags": {
                "rate": false,
                "group": ["instance"],
                "fill": true,
                "smooth": true,
                "smoothLevel": "5"
            }
        },
        {
            "name": "istio_request_count",
            "flags": {
                "rate": true,
                "group": ["instance"],
                "fill": true,
                "smooth": true,
                "smoothLevel": "5",
                "normalize": true
            }
        }
    ],
    "times": {
        "startTime": "now() - 1h",
        "stopTime": "now() - 0h"
        }
    }
